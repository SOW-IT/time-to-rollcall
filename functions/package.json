{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "npm run lint && tsc",
    "serve": "npm run build && firebase deploy --only functions",
    "ts:watch": "tsc --watch",
    "start:emulators": "firebase emulators:start --only functions",
    "dev": "concurrently --kill-others \"npm run start:emulators\" \"npm run ts:watch\"",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run dev",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.27.1",
    "cors": "^2.8.5",
    "firebase": "^10.5.2",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^4.2.1",
    "react-firebase-hooks": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^7.1.0",
    "eslint": "^8",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "private": true
}
